    <function name="account" type="None" variable="">
        <parameters>
            <parameter name="arr1" type="String" array="True"/>
            <parameter name="arr2" type="String" array="True"/>
            <parameter name="arrlen" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="chk" type="Integer" array="False" size=""/>
            <assign variable="chk" expression="1"/>
            <declare name="login, i, j, k, a, b" type="Integer" array="False" size=""/>
            <declare name="u, pwd, ch" type="String" array="False" size=""/>
            <assign variable="i" expression="0"/>
            <assign variable="b" expression="0"/>
            <assign variable="a" expression="0"/>
            <output expression="&quot;CUSTOMER&quot;&amp;TOCHAR(13)&amp;&quot;===========&quot;&amp;tochar(13)&amp;&quot;1. LOGIN (already a user ?)&quot;&amp;tochar(13)&amp;&quot;2. SIGN UP (new user ?)&quot;" newline="True"/>
            <input variable="login"/>
            <while expression="login!=1&amp;&amp;login!=2">
                <output expression="&quot;PLEASE enter a VALID option&quot;" newline="True"/>
                <input variable="login"/>
            </while>
            <if expression="login==1">
                <then>
                    <if expression="i==0">
                        <then>
                            <output expression="&quot;THERE ARE NO REGISTERED ACCOUNTS&quot;" newline="True"/>
                            <call expression="main"/>
                        </then>
                        <else/>
                    </if>
                    <output expression="&quot;LOGIN&quot;&amp;TOCHAR(13)&amp;&quot;======&quot;" newline="True"/>
                    <do expression="b==-1">
                        <if expression="b=-1">
                            <then>
                                <output expression="&quot;ENTER A VALID USERNAME AND PASSWORD :&quot;" newline="True"/>
                            </then>
                            <else/>
                        </if>
                        <output expression="&quot;ENTER YOUR USERNAME :&quot;" newline="True"/>
                        <input variable="u"/>
                        <for variable="j" start="0" end="arrlen-1" direction="inc" step="1">
                            <if expression="arr1[j]==u">
                                <then>
                                    <assign variable="a" expression="-1"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="a!=-1">
                            <then>
                                <output expression="&quot;ENTER A VALID USERNAME&quot;" newline="True"/>
                            </then>
                            <else/>
                        </if>
                        <output expression="&quot;ENTER YOUR PASSWORD :&quot;" newline="True"/>
                        <input variable="pwd"/>
                        <for variable="j" start="0" end="arrlen-1" direction="inc" step="1">
                            <if expression="arr1[j]==u">
                                <then>
                                    <if expression="arr2[j]==pwd">
                                        <then>
                                            <output expression="&quot;YOU have SUUCESSFULLY LOGGED in&quot;" newline="True"/>
                                            <assign variable="b" expression="0"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;USERNAME and PASSWORD does NOT match&quot;" newline="True"/>
                                            <assign variable="b" expression="-1"/>
                                        </else>
                                    </if>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="b==-1">
                            <then>
                                <assign variable="a" expression="-1"/>
                            </then>
                            <else/>
                        </if>
                    </do>
                </then>
                <else>
                    <declare name="mail, pwd1, mobile" type="String" array="False" size=""/>
                    <declare name="age" type="Integer" array="False" size=""/>
                    <output expression="&quot;SIGN UP&quot;&amp;TOCHAR(13)&amp;&quot;======&quot;" newline="True"/>
                    <output expression="&quot;ENTER YOUR USERNAME :&quot;" newline="True"/>
                    <input variable="arr1[i]"/>
                    <do expression="age&lt;=0">
                        <output expression="&quot;enter valid age&quot;" newline="True"/>
                        <input variable="age"/>
                    </do>
                    <do expression="(len(mail)&lt;=5)||(chk==1)">
                        <output expression="&quot;enter valid email address&quot;" newline="True"/>
                        <input variable="mail"/>
                        <assign variable="k" expression="0"/>
                        <while expression="k&lt;len(mail)">
                            <if expression="char(mail,k)==&quot;@&quot;">
                                <then>
                                    <assign variable="chk" expression="0"/>
                                </then>
                                <else/>
                            </if>
                            <assign variable="k" expression="k+1"/>
                        </while>
                    </do>
                    <do expression="(len(arr2[i])&lt;=8)||(len(arr2[i])&gt;=12)">
                        <output expression="&quot;ENTER YOUR PASSWORD :&quot;" newline="True"/>
                        <input variable="arr2[i]"/>
                    </do>
                    <do expression="pwd1!=arr2[i]">
                        <output expression="&quot;RE-ENTER YOUR PASSWORD :&quot;" newline="True"/>
                        <input variable="pwd1"/>
                    </do>
                    <declare name="FLAG" type="Integer" array="False" size=""/>
                    <assign variable="FLAG" expression="0"/>
                    <do expression="(FLAG==0)||(len(mobile)!=10)">
                        <output expression="&quot;ENTER MOBILE NUMBER&quot;" newline="True"/>
                        <input variable="mobile"/>
                        <for variable="i" start="0" end="len(mobile)-1" direction="inc" step="1">
                            <if expression="(char(mobile,i)&gt;=tochar(48))||(char(mobile,i)&lt;=tochar(57))">
                                <then>
                                    <assign variable="FLAG" expression="1"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                    </do>
                    <assign variable="i" expression="i+1"/>
                    <output expression="&quot;ACCOUNT CREATED SUCCESFULLY&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
