    <function name="billing" type="Real" variable="total">
        <parameters>
            <parameter name="customerbuffername" type="String" array="True"/>
            <parameter name="customerbufferamount" type="Real" array="True"/>
            <parameter name="customerbufferhotelname" type="String" array="True"/>
            <parameter name="customerbuffercost" type="Real" array="True"/>
            <parameter name="customerbfrlen" type="Integer" array="False"/>
            <parameter name="customerbufferqty" type="Integer" array="True"/>
            <parameter name="termkey" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="total" type="Real" array="False" size=""/>
            <assign variable="total" expression="0"/>
            <declare name="i, s, pos" type="Integer" array="False" size=""/>
            <assign variable="pos" expression="-1"/>
            <assign variable="s" expression="0"/>
            <output expression="&quot;=====================BILL=============================&quot;&amp;Tochar(13)&amp;&quot;      s.no   |   hotelname | foodname   |  cost  |  qty | amount&quot;" newline="True"/>
            <assign variable="i" expression="0"/>
            <for variable="i" start="0" end="customerbfrlen" direction="inc" step="1">
                <if expression="customerbuffercost[i]!=0">
                    <then>
                        <assign variable="pos" expression="i"/>
                    </then>
                    <else/>
                </if>
            </for>
            <for variable="i" start="0" end="pos" direction="inc" step="1">
                <output expression="i+1&amp;&quot; . &quot;&amp;customerbufferhotelname[i]&amp;&quot;| &quot;&amp;customerbuffername[i]&amp;&quot;| &quot;&amp;customerbuffercost[i]&amp;&quot;| &quot;&amp;customerbufferqty[i]&amp;&quot;| &quot;&amp;customerbuffercost[i]*customerbufferqty[i]" newline="True"/>
                <assign variable="total" expression="total+(customerbuffercost[i]*customerbufferqty[i])"/>
            </for>
            <do expression="(i&lt;=0)||(i&gt;2)">
                <output expression="&quot;to place your order press 1 to add items to your cart press 2&quot;" newline="True"/>
                <input variable="i"/>
            </do>
            <if expression="i==1">
                <then>
                    <assign variable="termkey[0]" expression="1"/>
                </then>
                <else>
                    <assign variable="termkey[0]" expression="0"/>
                </else>
            </if>
        </body>
