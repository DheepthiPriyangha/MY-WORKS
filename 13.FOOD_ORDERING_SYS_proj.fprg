            <parameter name="itemcostc3" type="Real" array="True"/>
            <parameter name="itemcostc4" type="Real" array="True"/>
        </parameters>
        <body>
            <declare name="choice, i, j, ch, s" type="Integer" array="False" size=""/>
            <assign variable="s" expression="1"/>
            <declare name="dishname" type="String" array="False" size=""/>
            <declare name="cost" type="Real" array="False" size=""/>
            <do expression="(choice&lt;=0)||(choice&gt;3)">
                <output expression="&quot;1.add&quot;&amp;Tochar(13)&amp;&quot;2.alter&quot;&amp;Tochar(13)&amp;&quot;3.delete&quot;" newline="True"/>
                <input variable="choice"/>
                <if expression="choice ==1">
                    <then>
                        <do expression="(i&lt;=0)||(i&gt;4)">
                            <output expression="&quot;enter to which cuisine you want to add&quot;" newline="False"/>
                            <output expression="tochar(13)&amp;  &quot;1.South Indian&quot; &amp;tochar(13)&amp; &quot;2.North India&quot; &amp;tochar(13)&amp;  &quot;3.Chinese or Italian&quot;  &amp;tochar(13)&amp; &quot;4.Fast foods and beverages&quot;" newline="True"/>
                            <input variable="i"/>
                        </do>
                        <if expression="i==1">
                            <then>
                                <if expression="dishc1[dishlenc1-1]==&quot; &quot;">
                                    <then>
                                        <output expression="&quot;enter name of dish item&quot;" newline="True"/>
                                        <input variable="dishname"/>
                                        <do expression="cost==0">
                                            <output expression="&quot;enter cost of dish&quot;" newline="True"/>
                                            <input variable="cost"/>
                                        </do>
                                        <assign variable="j" expression="0"/>
                                        <for variable="i" start="0" end="dishlenc1-1" direction="inc" step="1">
                                            <if expression="dishc1[i]!=&quot; &quot;">
                                                <then>
                                                    <assign variable="j" expression="i"/>
                                                </then>
                                                <else/>
                                            </if>
                                        </for>
                                        <assign variable="dishc1[j+1]" expression="dishname"/>
                                        <assign variable="itemcostc1[j+1]" expression="cost"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;stack overflow&quot;" newline="True"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <if expression="i==2">
                                    <then>
                                        <if expression="dishc2[dishlenc2-1]==&quot; &quot;">
                                            <then>
                                                <output expression="&quot;enter name of dish item&quot;" newline="True"/>
                                                <input variable="dishname"/>
                                                <do expression="cost==0">
                                                    <output expression="&quot;enter cost of dish&quot;" newline="True"/>
                                                    <input variable="cost"/>
                                                </do>
                                                <assign variable="j" expression="0"/>
                                                <for variable="i" start="0" end="dishlenc2-1" direction="inc" step="1">
                                                    <if expression="dishc2[i]!=&quot; &quot;">
                                                        <then>
                                                            <assign variable="j" expression="i"/>
                                                        </then>
                                                        <else/>
                                                    </if>
                                                </for>
                                                <assign variable="dishc2[j+1]" expression="dishname"/>
