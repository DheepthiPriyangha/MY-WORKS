    <function name="alterfooddetails" type="None" variable="">
        <parameters>
            <parameter name="dishc1" type="String" array="True"/>
            <parameter name="dishc2" type="String" array="True"/>
            <parameter name="dishc3" type="String" array="True"/>
            <parameter name="dishc4" type="String" array="True"/>
            <parameter name="hotelc1" type="String" array="True"/>
            <parameter name="hotelc2" type="String" array="True"/>
            <parameter name="hotelc3" type="String" array="True"/>
            <parameter name="hotelc4" type="String" array="True"/>
            <parameter name="dishlenc1" type="Integer" array="False"/>
            <parameter name="dishlenc2" type="Integer" array="False"/>
            <parameter name="dishlenc3" type="Integer" array="False"/>
            <parameter name="dishlenc4" type="Integer" array="False"/>
            <parameter name="hotellenc1" type="Integer" array="False"/>
            <parameter name="hotellenc2" type="Integer" array="False"/>
